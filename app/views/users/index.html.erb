<main>
<p id="notice"></p>
  <div class="container px-5 px-sm-0">
    <div class="row">
      <div class="col-md-3">
      <!--部分テンプレート読み込み-->
      <%= render 'books/list', books: @books %>
    </div>

    <div class='col-md-8 offset-md-1'>
       <h2>Users</h2>
         <table class='table'>
          <thead>
            <tr>
              <th>image</th>
              <th>name</th>
              <th colspan="3"></th>
            </tr>
          </thead>
         <tbody>
          <% provide(:title, 'All users') %>
          <ul class="users">
          <% @users.each do |user| %>
           <tr>
             <td>image</td>
             <td><%= user.name %></td>
             <td><%= link_to 'show', user %></td>
           </tr>
          <% end %>
          </ul>
        </table>
       
    </div>
  </div>
</main>